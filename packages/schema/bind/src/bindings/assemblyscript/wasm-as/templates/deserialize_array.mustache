{{#scalar}}
return reader.read{{#toMsgPack}}{{toGraphQLType}}{{/toMsgPack}}();
{{/scalar}}
{{#array}}
return reader.read{{#toMsgPack}}{{toGraphQLType}}{{/toMsgPack}}((reader: Read): {{#item}}{{#toWasm}}{{toGraphQLType}}{{/toWasm}}{{/item}} => {
  {{> deserialize_array}}
});
{{/array}}
{{#enum}}
{{> deserialize_enum}}
return value;
{{/enum}}
{{#union}}
{{> deserialize_union}}
return union;
{{/union}}
{{#object}}
{{> deserialize_object}}
return object;
{{/object}}
